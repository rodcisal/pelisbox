<%- model_class = Pelicula -%>
<div class="page-header">
  <h1><strong><%= @pelicula.titulo %></strong></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:titulo) %>:</strong></dt>
  <dd><%= @pelicula.titulo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:descripcion) %>:</strong></dt>
  <dd><%= @pelicula.descripcion %></dd>
  <dd><%= raw @pelicula.iframe %></dd>
  <dt><strong><%= model_class.human_attribute_name(:categoria) %>:</strong></dt>
  <dd><%= @pelicula.categoria %></dd>
</dl>


